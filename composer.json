{
  "name": "sinema/data-entities",
  "description": "",
  "keywords": [
    "Data Transfer Objects"
  ],
  "homepage": "https://github.com/SineMah/data-entities",
  "license": "MIT",
  "authors": [
    {
      "name": "Sinema"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^8.1.0"
  },
  "autoload": {
    "psr-4": {
      "Sinemah\\DataEntities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sinemah\\DataEntities\\Tests\\": "tests/TestClasses"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests/",
    "testdox": "./vendor/bin/phpunit --bootstrap vendor/autoload.php --testdox --colors=always tests/"
  },
  "minimum-stability": "dev"
}
